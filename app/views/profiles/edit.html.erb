<%= head_page "edit_profile" %>
<% semantic_form_for :profile, @profile, :url => profile_path, :html => { :method => :put, :class => 'base_form' }  do |form| %>
<%= render :partial => 'form', :object => form %>
<% form.buttons do -%>
<%= form.commit_button I18n.t('save'),  :class => "base_buttons" %>
<% end -%>
<% end -%>
